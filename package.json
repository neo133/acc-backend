{
  "name": "au-api",
  "version": "1.0.0",
  "description": "AU API server",
  "main": "index.js",
  "author": "Akshay Chaturvedi",
  "license": "ISC",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start processes.json dist/index.bundle.js",
    "dev": "concurrently --kill-others \"yarn dev:watch\" \"yarn dev:start\"",
    "dev:debug": "concurrently --kill-others \"yarn dev:watch\" \"yarn dev:start:debug\"",
    "dev:start": "cross-env NODE_ENV=development nodemon dist/index.bundle.js",
    "dev:start:debug": "cross-env NODE_ENV=development MONGOOSE_DEBUG=true DEBUG=express:* nodemon dist/index.bundle.js",
    "dev:watch": "webpack --watch --mode=development",
    "build": "rimraf dist && webpack --mode=production",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bcrypt-nodejs": "^0.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.8",
    "express-winston": "^4.0.3",
    "helmet": "^3.22.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^1.16.0",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pretty-error": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sequelize": "^6.5.1",
    "sequelize-auto": "^0.8.1",
    "socket.io": "^4.4.1",
    "winston": "^3.4.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ignore-styles": "^5.0.1",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  }
}
